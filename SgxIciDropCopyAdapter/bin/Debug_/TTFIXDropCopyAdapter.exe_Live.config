<?xml version="1.0" encoding="utf-8"?>
<configuration>  
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
  </configSections>
  <connectionStrings>
    <add name="myConn" connectionString="Data Source=ASHOK;Initial Catalog=BackOfficeTest;user= sa; password = sa@123;"/>
  </connectionStrings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2"/>
  </startup>
  <appSettings>
    <add key="DropCopy_IP" value="127.0.0.1"/>
    <add key="DropCopy_Port" value="11701"/>
    <add key="SenderCompId" value="AA_DC2"/>
    <add key="TargetCompId" value="TT_Order"/>
    <add key="SenderSubId" value=""/>
    <add key="HeartbeatInterval" value="30"/>
    <add key="Password" value="Rome2019"/>
    <add key="BeginString" value="FIX.4.2"/>
    <add key="IsPersistant" value="True"/>
  </appSettings>
  <log4net>
    <appender name="LogFileAppender" type="log4net.Appender.RollingFileAppender">
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
      <file value="logs\AppLog_"/>
      <datePattern value="dd.MM.yyyy'.log'"/>
      <staticLogFileName value="false"/>
      <rollingStyle value="Composite"/>
      <appendToFile value="true"/>      
      <maxSizeRollBackups value="10"/>
      <maximumFileSize value="10MB"/>      
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="[%d{yyyy-MM-dd HH:mm:ss}] %logger - [%-5p] – %m%n"/>
      </layout>
    </appender>
    <appender name="ErrorFileAppender" type="log4net.Appender.RollingFileAppender">
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
      <file value="logs\Error_"/>
      <datePattern value="dd.MM.yyyy'.log'"/>
      <staticLogFileName value="false"/>
      <rollingStyle value="Composite"/>
      <appendToFile value="true"/>
      <maxSizeRollBackups value="10"/>
      <maximumFileSize value="10MB"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="[%d{yyyy-MM-dd HH:mm:ss}] %logger - [%-5p] – %m%n"/>
      </layout>
    </appender>
     <appender name="FixFileAppender" type="log4net.Appender.RollingFileAppender">
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
      <file value="logs\FixData_"/>
      <datePattern value="dd.MM.yyyy'.log'"/>
      <staticLogFileName value="false"/>
      <rollingStyle value="Composite"/>
      <appendToFile value="true"/>
      <maxSizeRollBackups value="10"/>
      <maximumFileSize value="10MB"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="[%d{yyyy-MM-dd HH:mm:ss}] %logger - [%-5p] – %m%n"/>
      </layout>
    </appender>
    <logger name="LogFileAppender" additivity="false">
      <level value="Info"/>
      <appender-ref ref="LogFileAppender"/>
    </logger>
    <logger name="ErrorFileAppender" additivity="false">
      <level value="Error"/>
      <appender-ref ref="ErrorFileAppender"/>
    </logger>
    <logger name="FixFileAppender" additivity="false">
      <level value="Debug"/>
      <appender-ref ref="FixFileAppender"/>
    </logger>
    <!--<root>
      <level value="ALL" />
      <appender-ref ref="LogFileAppender" />
      --><!--<appender-ref ref="ConsoleAppender" />--><!--
    </root>-->
  </log4net>
</configuration>
